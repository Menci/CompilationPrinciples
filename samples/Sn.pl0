var a, b, n, sum;

begin
    read(a);
    read(n);
    if n > 0 then
        if n < 10 then
            begin
                b := a;
                sum := 0;
                repeat
                    sum := sum + b;
                    b := b * 10 + a;
                    n := n - 1;
                until n = 0;
            end;
    write(sum);
end.
